&lt;p&gt;&lt;br&gt;&lt;span&gt;What attribute prevents the first frame of the video from displaying while the video is downloading?&lt;/span&gt;&lt;/p&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="4259">
                <material>
                  <mattext texttype="text/plain">The poster attribute</mattext>
                </material>
              </response_label>
              <response_label ident="4333">
                <material>
                  <mattext texttype="text/plain">The controls attribute</mattext>
                </material>
              </response_label>
              <response_label ident="2455">
                <material>
                  <mattext texttype="text/plain">The src attribute</mattext>
                </material>
              </response_label>
              <response_label ident="8481">
                <material>
                  <mattext texttype="text/plain">The type attribute</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
