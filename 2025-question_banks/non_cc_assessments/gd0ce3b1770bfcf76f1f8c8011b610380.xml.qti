    <item ident="ge747a67b3e8bce8e456e07d6bd271d37" title="Question">
      <itemmetadata>
        <qtimetadata>
          <qtimetadatafield>
            <fieldlabel>question_type</fieldlabel>
            <fieldentry>multiple_choice_question</fieldentry>
          </qtimetadatafield>
          <qtimetadatafield>
            <fieldlabel>points_possible</fieldlabel>
            <fieldentry>1.0</fieldentry>
          </qtimetadatafield>
          <qtimetadatafield>
            <fieldlabel>original_answer_ids</fieldlabel>
            <fieldentry>3098,8890,4232,8678</fieldentry>
          </qtimetadatafield>
        </qtimetadata>
      </itemmetadata>
      <presentation>
        <material>
          <mattext texttype="text/html">&lt;div&gt;&lt;span class="_title"&gt;Which statement is true about the security of the JavaScript language?&lt;/span&gt;&lt;span&gt; &lt;/span&gt;&lt;/div&gt;</mattext>
        </material>
        <response_lid ident="response1" rcardinality="Single">
          <render_choice>
            <response_label ident="3098">
              <material>
                <mattext texttype="text/plain">JavaScript has some security features that can help protect users from malicious Web sites by enforcing strict limits on the page authors.</mattext>
              </material>
            </response_label>
            <response_label ident="8890">
              <material>
                <mattext texttype="text/plain">Client-side JavaScript cannot be easily viewed in the browser (like X/HTML) and is therefore useful for collecting or validating user names or passwords.</mattext>
              </material>
            </response_label>
            <response_label ident="4232">
              <material>
                <mattext texttype="text/plain">JavaScript has no security features and presents many risks to both developers and end-users.</mattext>
              </material>
            </response_label>
            <response_label ident="8678">
              <material>
                <mattext texttype="text/plain">JavaScript protects data passed between browser and server, and can also protect Web site owners from manipulation of their pages by outsiders</mattext>
              </material>
            </response_label>
          </render_choice>
        </response_lid>
      </presentation>
      <resprocessing>
        <outcomes>
          <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
        </outcomes>
        <respcondition continue="No">
          <conditionvar>
            <varequal respident="response1">3098</varequal>
          </conditionvar>
          <setvar action="Set" varname="SCORE">100</setvar>
        </respcondition>
      </resprocessing>
    </item>
    <item ident="g0a333c2770dceed0642f20842250f9e0" title="Question">
