      </itemmetadata>
      <presentation>
        <material>
          <mattext texttype="text/html">&lt;div&gt;&lt;span class="_title"&gt;In regard to security, JavaScript:&lt;/span&gt;&lt;span&gt; &lt;/span&gt;&lt;/div&gt;</mattext>
        </material>
        <response_lid ident="response1" rcardinality="Single">
          <render_choice>
            <response_label ident="1828">
              <material>
                <mattext texttype="text/plain">is a fairly secure open scripting language.</mattext>
              </material>
            </response_label>
            <response_label ident="578">
              <material>
                <mattext texttype="text/plain">can replace many common security measures, such as encryption.</mattext>
              </material>
            </response_label>
            <response_label ident="8699">
              <material>
                <mattext texttype="text/plain">is safe enough to collect or validate user names or passwords.</mattext>
              </material>
            </response_label>
            <response_label ident="5508">
              <material>
                <mattext texttype="text/plain">cannot be viewed in the browser by end-users.</mattext>
              </material>
            </response_label>
          </render_choice>
        </response_lid>
      </presentation>
      <resprocessing>
        <outcomes>
          <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
        </outcomes>
        <respcondition continue="No">
          <conditionvar>
            <varequal respident="response1">1828</varequal>
          </conditionvar>
          <setvar action="Set" varname="SCORE">100</setvar>
        </respcondition>
