              <response_label ident="7107">
                <material>
                  <mattext texttype="text/plain">The if statement</mattext>
                </material>
              </response_label>
              <response_label ident="2244">
                <material>
                  <mattext texttype="text/plain">The while statement</mattext>
                </material>
              </response_label>
              <response_label ident="1960">
                <material>
                  <mattext texttype="text/plain">The if...else statement</mattext>
                </material>
              </response_label>
              <response_label ident="9089">
                <material>
                  <mattext texttype="text/plain">The for statement</mattext>
                </material>
